<h1>@ViewBag.title</h1>
@if (ViewBag.column == "all")
 {
    @foreach (var row in ViewBag.jobs)
      {
<table class="job-listing">
    @foreach (KeyValuePair<string,string> job in row)
        {
        <tr>
            <td>@job.Key</td>
            <td>@job.Value</td>
        </tr>
        }
</table>
    }

 }

<table>
    @for (int i = 0; i < ViewBag.items.Count; i++)
    {
        <tr class="job-listing">
            <td><a asp-controller="List" asp-action="Jobs" asp-route-column="@ViewBag.column" asp-route-value="@ViewBag.items[i]">@ViewBag.items[i]</a></td>
        </tr>
    }
</table>
